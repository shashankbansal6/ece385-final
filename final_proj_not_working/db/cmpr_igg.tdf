--synthesis_resources = lut 16 
SUBDESIGN cmpr_igg
( 
	alb	:	output;
	dataa[15..0]	:	input;
	datab[15..0]	:	input;
) 
VARIABLE
	alb_int	:	WIRE;
	dataa_int[15..0]	:	WIRE;
	datab_int[15..0]	:	WIRE;
BEGIN 
	dataa_int[] = ( !dataa[15] , dataa[14..0]);
	datab_int[] = ( !datab[15] , datab[14..0]);
	IF (dataa_int[] < datab_int[]) THEN
		alb_int = VCC;
	ELSE
		alb_int = GND;
	END IF;
	alb = alb_int;
END;
--VALID FILE
