FUNCTION accum_9ml (aclr, clken, clock, data[31..0], sign_data)
RETURNS ( result[32..0]);

--synthesis_resources = lut 33 reg 33 
SUBDESIGN zaccum_efi
( 
	aclr	:	input;
	clken	:	input;
	clock	:	input;
	data[31..0]	:	input;
	result[32..0]	:	output;
	sign_data	:	input;
) 
VARIABLE 
	accum : accum_9ml;

BEGIN 
	accum.aclr = aclr;
	accum.clken = clken;
	accum.clock = clock;
	accum.data[] = data[];
	accum.sign_data = sign_data;
	result[] = accum.result[];
END;
